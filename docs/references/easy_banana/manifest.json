{
  "manifest_version": 3,
  "name": "Easy Banana",
  "version": "2.10.0",
  "description": "Side panel that shows 'Easy Banana'.",
  "action": {
    "default_title": "Open Easy Banana",
    "default_icon": {
      "16": "icons/icon128.png",
      "32": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon128.png",
    "32": "icons/icon128.png",
    "48": "icons/icon128.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "system.display",
    "windows",
    "downloads",
    "tabs"
  ],
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*",
    "https://*/*"
  ]
}
